#: prefs.js:150 prefs.js:632 prefs.js:1243 prefs.js:2199
msgid "Dash"
msgstr "Dash"

#: prefs.js:156
msgid "Dash Position"
msgstr "Posizione dash"

#: prefs.js:161 prefs.js:420
msgid "Bottom"
msgstr "In basso"

#: prefs.js:162
msgid "Left"
msgstr "Sinistra"

#: prefs.js:163
msgid "Top"
msgstr "In alto"

#: prefs.js:164
